<% include header %>
<div id="container" class="container">
  <div class="row">
    <div class="col-md-9 col-sm-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          <ol class="breadcrumb">
            <li><a href="#">主页</a></li>
            <li class="active">注册</li>
          </ol>
        </div>
        <div class="panel-body">
          <div class="row">
            <div class="col-md-7">
              <form method="post" action="/signup" class="form-horizontal">
                <div class="form-group">
                  <label for="tUser" class="col-sm-3 control-label">用户名:</label>
                  <div class="col-sm-9">
                    <input class="form-control" type="text" name="tUser" id="tUser" placeholder="用户名">
                  </div>
                </div>
                <div class="form-group">
                  <label for="tPass" class="col-sm-3 control-label">密 码：</label>
                  <div class="col-sm-9">
                    <input class="form-control" type="text" name="tPass" id="tPass" placeholder="密码">
                  </div>
                </div>
                <div class="form-group">
                  <label for="tpass2" class="control-label col-sm-3">确认密码：</label>
                  <div class="col-sm-9">
                    <input class="form-control" type="password" name="tPass2" id="tPass2" placeholder="确定密码">
                  </div>
                </div>
                <div class="form-group">
                  <label for="temail" class="control-label col-sm-3">邮 箱：</label>
                  <div class="col-sm-9">
                    <input class="form-control" type="email" name="temail" id="temail" placeholder="email">
                  </div>
                </div>
                <div class="form-group">
                  <div class="col-sm-offset-3 col-sm-9">
                    <button type="submit" class="btn btn-primary">登 录</button>
                    <button type="button" class="btn btn-info">取 消</button>
                  </div>
                </div>
              </form>
            </div>
            <div class="col-md-5"></div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-md-3 col-sm-12">
      <% include siderabout %>
    </div>
  </div>
</div>
<% include footer %>
